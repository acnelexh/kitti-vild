#!/bin/bash

DATA_DIR=./data/kitti/training/image_2
DEST_DIR=./tmp
VAL_JSON="./sample.json"
python3 preprocessing/create_kitti_tf_record.py \
  --image_dir="${DATA_DIR}" \
  --json_path="${VAL_JSON}" \
  --include_mask=True \
  --split='val' \
  --num_parts=5 \
  --max_num_processes=1 \
  --dest_dir="${DEST_DIR}"

# The script create 5 tf record with 1 process